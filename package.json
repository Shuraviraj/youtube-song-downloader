{
  "name": "youtube-mp3-downloader",
  "version": "1.0.0",
  "description": "YouTube MP3 Downloader - Educational Full Stack Project",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:backend": "cd backend && npm start",
    "start:frontend": "ng serve",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "setup": "npm install && cd backend && npm install",
    "clean": "rm -rf node_modules backend/node_modules dist .angular/cache"
  },
  "keywords": [
    "youtube",
    "mp3",
    "downloader",
    "angular",
    "nodejs",
    "educational"
  ],
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.6",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "concurrently": "^9.2.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}